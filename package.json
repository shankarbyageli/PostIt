{
  "name": "PostIt",
  "version": "1.0.0",
  "description": "This is a content sharing website where you can share your post with others",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "export db=postitDB.db;  ./shell_scripts/setDb.sh database/postitDB.db; node server.js",
